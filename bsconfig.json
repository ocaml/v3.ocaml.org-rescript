{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "ocamlorg",
  "namespace": true,
  "reason": {
    "react-jsx": 3
  },
  "refmt": 3,
  "bs-dependencies": [
    "@rescript/react",
    "@glennsl/bs-json",
    "ood"
  ],
  "ppx-flags": [],
  "sources": [
    {
      "dir": "src"
    },
    {
      "dir": "pages",
      "subdirs": true
    }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".mjs",
  "warnings": {
    "error": "+101+8"
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-g"
  ]
}
