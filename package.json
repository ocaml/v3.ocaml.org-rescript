  "engines": {
    "node": ">=12.x"
  },
  "enginesDescription": {
    "node": "change back to >=14.15.1 once vercel build is enhanced"
  },
  "dependencies": {
    "bs-platform": "8.3.1",
    "next": "^9.5.5",
    "next-mdx-remote": "^1.0.1",
    "next-transpile-modules": "^4.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "reason-react": "^0.9.1"
  },
  "dependenciesDescription": {
    "bs-platform": "This a regular dependency because the included standard libraries are used during runtime",
    "next-mdx-remote": "This allows for directly invoking mdxjs markdown conversion during nextjs data fetching"
  },
  "scripts": {
    "next:dev": "next dev",
    "next:debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "bsb -clean-world -make-world && next build",
    "next:export": "next export",
    "next:start": "next start -p $PORT",
    "rescript:build": "bsb -make-world",
    "rescript:clean": "bsb -clean-world",
    "rescript:start": "bsb -make-world -w"
  },
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "gentype": "^3.39.0",
    "postcss-cli": "7.1.2",
    "tailwindcss": "1.8.10",
    "yarn": "1.22"
  },
  "postcss": {
    "plugins": {
      "tailwindcss": {},
      "autoprefixer": {}
    }
  }
